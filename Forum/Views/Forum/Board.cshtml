@model IEnumerable<DataLayer.Board>

@{
    ViewBag.Title = "Board";
    Layout = "~/Views/Shared/menu.cshtml";
}

<h2>Board</h2>

<p>
    @{
        if (Session["user"] != null && Session["usertype"].ToString()=="MasterAdmin")
        { 
           @Html.ActionLink("Create New", "Create")
        }
    }
</p>
<table class="table ">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.BoardTitle)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            <!--@Html.DisplayFor(modelItem => item.BoardTitle)-->
            @Html.ActionLink(item.BoardTitle.ToString(), "Details", new { id=item.BoardId})
        </td>
        <td>
            <!--@Html.ActionLink("Edit", "Edit", new { id=item.BoardId }) 
            @Html.ActionLink("Details", "Details", new { id=item.BoardId }) 
            @Html.ActionLink("Delete", "Delete", new { id=item.BoardId })-->
        </td>
    </tr>
}

</table>